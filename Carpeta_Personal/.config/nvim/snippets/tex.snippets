# -- SNIPPETS --
#
# new glossary entry
snippet gloss "Glossary Entry"  b
	\newglossaryentry{${1:entry}}{
			name = {${2:name}},
			symbol = {\ensuremath{${3:symbol}}},
			description = {${4:description}}
	}
#

# \begin{}...\end{}
snippet begin "Begin End"
	\begin{${1}}
		$0	
	\end{$1}
#

# Align
snippet align "Align Env" b
	\begin{align$1}
		$0
	\end{align$1}
#

# Enumerate
snippet enumerate "Enumerate" b
	\begin{enumerate}
		\item $0
	\end{enumerate}
#

# Itemize
snippet itemize "Itemize" b
	\begin{itemize}
		\item $0
	\end{itemize}
#

# Cases
snippet casos "Entorno de Casos"
	${1:LHS}=	
	\begin{cases}
		${2:Value}, &\text{ ${3:coment} } ${4:case} \text{,} \\\\
		${5:Value}, &\text{ ${6:comment} } ${7:otherwise} .
	\end{cases}${0}
#

# Entorno de imagen
snippet figure "Figure environment" b
	\begin{figure}
	\begin{center}
		\includegraphics[scale=${1}]{Figures/${2}}
	\end{center}
	\caption{${3}}
	\label{fig:${4}}
	\end{figure}
	${0}
#

snippet includegraphics "Includegraphics enviroment" bi
	\begin{center}
		\includegraphics[scale=$1]{$2}
	\end{center}
	$0

snippet lemma "Lemma environment" bi
	\begin{lemma}{$1}
		$0
	\end{lemma}

snippet proposition "Proposition environment" bi
	\begin{proposition}{$1}
		$0
	\end{proposition}

snippet theorem "Theorem environment" bi
	\begin{theorem}{$1}
		$0
	\end{theorem}

snippet proof "Proof environment" bi
	\begin{proof} 
		$0
	\end{proof}

snippet definition "Definition environment bi
	\begin{definition}{$1}
		$0
	\end{definition}

snippet note "Note environment" bi
	\begin{note}{$1}
		$0
	\end{note}

snippet notation "Notation environment" bi
	\begin{notation}{$1}
		$0
	\end{notation}

snippet corollary "Corollary environment" bi
	\begin{corollary}{$1}
		$0
	\end{corollary}

snippet example "Example environment" bi
	\begin{example}{$1}
		$0
	\end{example}

snippet solution "Solution environment" bi
